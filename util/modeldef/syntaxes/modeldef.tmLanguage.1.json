{
	"name": "BCMD modeldef",
	"patterns": [
		{
			"include": "#strings"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {

		"strings": {
			"name": "string.quoted.double.modeldef",
			"begin": "\"",
			"end": "\"",
			"patterns": [{
				"name": "constant.character.escape.modeldef",
				"match": "\\\\."
			}]
		},
		"comments": {
			"name": "comment.line.number-sign.modeldef",
			"begin": "#",
			"end": "$"
		}
	},
	"scopeName": "text.modeldef"
}